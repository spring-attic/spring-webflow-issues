<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.springframework.org/schema/webflow https://www.springframework.org/schema/webflow/spring-webflow.xsd"
	  parent="common">

	<view-state id="view1">
		<transition on="continue" to="view2" />
	</view-state>

	<view-state id="view3">
		<transition on="continue" to="done" />
	</view-state>

	<end-state id="done" view="externalRedirect:contextRelative:/" />

</flow>